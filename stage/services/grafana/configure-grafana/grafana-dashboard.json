      "targets": [
        {
          "format": "table",
          "group": [],
          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "select case \r\n            when TIMESTAMPDIFF(HOUR,t.created,ifnull(t.closed,now())) <= 02 then '0. Up to 2 Hours'\r\n            when TIMESTAMPDIFF(HOUR,t.created,ifnull(t.closed,now())) <= 04 then '1. Up to 4 Hours'\r\n            when TIMESTAMPDIFF(HOUR,t.created,ifnull(t.closed,now())) <= 08 then '2. Up to 8 Hours'\r\n            when TIMESTAMPDIFF(HOUR,t.created,ifnull(t.closed,now())) <= 24 then '3. Up to 24 Hours'\r\n            when TIMESTAMPDIFF( DAY,t.created,ifnull(t.closed,now())) <= 02 then '4. Up to 2 Days'\r\n            when TIMESTAMPDIFF( DAY,t.created,ifnull(t.closed,now())) <= 07 then '5. Up to 7 Days'\r\n            when TIMESTAMPDIFF( DAY,t.created,ifnull(t.closed,now()))  > 07 then '6. Greater than 8 Days'\r\n          end \"Creation Age\"\r\n      , count(*) Qty\r\n  from ost_ticket t\r\n where closed is null AND status_id IN (1,2,7)\r\n group by 1",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "timeFrom": null,
