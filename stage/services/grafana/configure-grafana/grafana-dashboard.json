          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "select (sum(case when t.created + interval s.grace_period hour <= now() then 0 else 1 end) / count(*)) * 100 pct_sla_on_time\r\n  from ost_ticket t\r\n  left join ost_sla s\r\n    on s.id = t.sla_id\r\n where closed is null\r\n",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
